public class ImprimirNumerosDivisibles {

    public static void main(String[] args) {
        int i;
        
        System.out.println("NÃºmeros divisibles por 3 y 5 entre 1 y 100:");
        for (i = 1; i <= 100; i++) {
            if (i % 3 == 0 && i % 5 == 0) {
                System.out.println(i);
            }
        }
    }
}
