public class Areaderectangulo {

    public static void main(String[] args) {
        
         Scanner teclado = new Scanner(System.in);
        System.out.print("Ingrese la longitud del rect치ngulo: ");
        double longitud = teclado.nextDouble();
        System.out.print("Ingrese la anchura del rect치ngulo: ");
        double anchura = teclado.nextDouble();

        double area = longitud * anchura;
        System.out.println("El 치rea del rect치ngulo es: " + area);
    }
}
